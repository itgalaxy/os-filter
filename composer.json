{
    "name": "itgalaxy/os-filter",
    "description": "Filter an array of objects to a specific OS",
    "keywords": [
        "arch",
        "filter",
        "obj",
        "object",
        "os"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Itgalaxy",
            "email": "info@itgalaxy.company"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.0",
        "phpunit/php-code-coverage": "^4.0.0",
        "satooshi/php-coveralls": "^1.0.0"
    },
    "scripts": {
        "phpunit": "phpunit",

        "test": "@phpunit"
    },
    "autoload": {
        "psr-4": {
            "Itgalaxy\\OsFilter\\": "src/"
        }
    },
    "archive": {
        "exclude": [
            "!vendor",
            "tests",
            "*phpunit.xml*",
            ".travis.yml",
            "appveyor.yml",
            "composer.phar"
        ]
    }
}
